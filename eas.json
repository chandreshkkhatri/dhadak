{
  "cli": {
    "version": ">= 3.10.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "ENVIRONMENT": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "ENVIRONMENT": "preview"
      }
    },
    "staging": {
      "channel": "staging",
      "env": {
        "ENVIRONMENT": "staging"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
